<Application
    x:Class="FocusTask.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:controls="using:Microsoft.UI.Xaml.Controls"
    xmlns:local="using:FocusTask">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="ms-appx:///Uwp.Core/Converter/Converter.xaml"/>
                <ResourceDictionary Source="ms-appx:///Style/String.xaml"/>
                <ResourceDictionary Source="ms-appx:///Style/Color.xaml"/>
                <controls:XamlControlsResources />
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
